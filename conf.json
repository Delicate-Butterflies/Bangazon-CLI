{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "app/models/Order-Product.js",
      "app/models/Order.js",
      "app/models/Payment-Types.js",
      "app/models/Product-Type.js",
      "app/models/Product.js",
      "app/models/User.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "destination": "./docs/" // same as -d ./docs/
  }
}
